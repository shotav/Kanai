{
  "name": "kanai",
  "version": "1.0.0",
  "description": "A lightweight cross-platform code editor.",
  "keywords": [
    "cross-platform",
    "code-editor"
  ],
  "homepage": "https://github.com/PryosCode/Kanai",
  "license": "Apache-2.0",
  "private": true,
  "author": {
    "name": "PryosCode",
    "email": "info@pryoscode.net",
    "url": "https://pryoscode.net"
  },
  "bugs": {
    "url": "https://github.com/PryosCode/Kanai/issues",
    "email": "info@pryoscode.net"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PryosCode/Kanai.git"
  },
  "scripts": {
    "dev:compile": "go build -o dist/kanai src/main/main.go",
    "dev:bundle": "webpack serve --mode=development",
    "dev:open": "wait-on http://localhost:8080 && ./dist/kanai",
    "dev": "yarn dev:compile && concurrently -k \"yarn dev:bundle\" \"yarn dev:open\"",
    "build:bundle": "webpack --mode=production",
    "build:compile": "",
    "build": "yarn build:bundle && yarn build:compile"
  },
  "engines": {
    "node": ">=14.0.0",
    "yarn": ">=1.0.0 <2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "autoprefixer": "^10.3.7",
    "babel-loader": "^8.2.3",
    "concurrently": "^6.3.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "cssnano": "^5.0.8",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "style-loader": "^3.3.1",
    "wait-on": "^6.0.0",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1"
  }
}